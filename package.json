    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "^4.0.0",
    "express": "^4.16.2",
    "jquery": "^3.3.1",
    "js-cookie": "^2.2.0",
    "laravel-mix": "^2.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "popper.js": "^1.13.0",
    "vue": "^2.5.13",
    "vue-events": "^3.1.0",
    "vue-i18n": "^7.4.1",
    "vue-meta": "^1.4.2",
    "vue-progressbar": "^0.7.3",
